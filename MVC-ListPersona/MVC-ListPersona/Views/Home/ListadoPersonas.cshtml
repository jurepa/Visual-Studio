@model MVC_ListPersona.Models.ListPersona
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListadoPersonas</title>
</head>
<body>
    <div>
        <ou>
            @for (int i = 0; i < Model.size(); i++)
            {
                <li>ID: @Model.getPersona(i).idPersona</li>
                <li>Nombre: @Model.getPersona(i).nombre</li>
                <li>Apellidos: @Model.getPersona(i).apellidos</li>
                <li>Direccion: @Model.getPersona(i).direccion</li>
                <li>Telefono: @Model.getPersona(i).telefono</li>
                <br />
                <br />
            }
        </ou>
    </div>
</body>
</html>
